<div class="chatbox chatbox--tray">
  <div class="chatbox__title">
      <h5>Product Adviser</h5>
	  <button class="chatbox__title__closex " onclick="toggleNavScuffed()">
          <span>
              <svg viewBox="0 0 12 12" width="12px" height="12px">
                  <line stroke="#FFFFFF" x1="11.75" y1="0.25" x2="0.25" y2="11.75"></line>
                  <line stroke="#FFFFFF" x1="11.75" y1="11.75" x2="0.25" y2="0.25"></line>
              </svg>
          </span>
      </button>
      <button class="chatbox__title__close">
          <span>
              <svg viewBox="0 0 12 12" width="12px" height="12px">
                  <line stroke="#FFFFFF" x1="11.75" y1="0.25" x2="0.25" y2="11.75"></line>
                  <line stroke="#FFFFFF" x1="11.75" y1="11.75" x2="0.25" y2="0.25"></line>
              </svg>
          </span>
      </button>
  </div>
  <div class="chatbox__body" id="message-container">
      <p class="small font-italic text-muted">Ask me anything</p>
  </div>
  {{!-- <textarea class="chatbox__message" placeholder="Write something interesting"></textarea> --}}
  <div class="chatbox__bottom">
  <form class="bg-light" id="bot-container">
        <div class="input-group" id="messageContainer">
          <input type="text" placeholder="Type a message" class="form-control rounded-0 border-0 py-4 bg-light" id='message-input' value="">
          <input type="hidden" id="userid" value="{{userid}}">
          <div class="input-group-append">
            <button id="send-button" type="submit" class="btn btn-link" style="z-index: 1;"> <i class="fa fa-paper-plane"></i></button>
          </div>
        </div>
      </form>
    </div>
</div>
<button id="chatbotToggle"><i class="fas fa-comments"></i></button>
<button id="chatbotTogglex" onclick="toggleNavScuffed()"><i class="fas fa-comments"></i></button>
